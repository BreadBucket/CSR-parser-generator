#if CSR_CODE
#include <stdio.h>

void nl();

void f(const char* name, CSRToken** t, int matched, int buffered){
	printf("\t=== [ inline code ] ===\n");
	
	printf("\tReached %s. Matched %d tokens. Buffered %d tokens.\n", name, matched, buffered);
	
	for (int i = 0 ; i < matched ; i++){
		printf("\tMatched: %d", t[i]->id);
		nl();
	}
	
	printf("\t=======================\n");
}

